programa {
  funcao inicio() {

    real num
    logico teste


    escreva("Digite o númeor para ver se ele é MAIOR QUE 10: ")
    leia(num)

    se (num>10)
    {
      teste = (num>10)
      escreva(teste)
    } senao se (num==10)
    {
      escreva("Espertinho você hein, pois o número É IGUAL a 10")
    } senao
    {
      teste = (num>10)
      escreva(teste)
    }
  }
}
